{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#458588",
    "green": "#8ec07c",
    "yellow": "#d79921",
    "red": "#cc241d",
    "dark": "#3c3836",
    "white": "#ffecc7",
    "os": "p:blue",
    "closer": "p:blue"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:os",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "mint": "\uf30f",
            "macos": "\uf179",
            "elementary": "\uF309",
            "raspbian": "\uF315",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\ue70f",
            "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}"
          },
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "properties": {
            "display_host": true
          },
          "style": "diamond",
          "template": "{{.UserName}} ",
          "trailing_diamond": "<p:blue>\uf101</> ",
          "type": "session"
        },
        {
          "foreground": "p:yellow",

          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " \uebcb ",
            "home_icon": "home",
            "style": "agnoster"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }} ",
          "trailing_diamond": "<p:yellow>\uf101</>",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "p:white",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffe600a4{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff990050{{ end }}",
            "{{ if gt .Ahead 0 }}#5d00ff3a{{ end }}",
            "{{ if gt .Behind 0 }}#5d00ff3a{{ end }}"
          ],
          "trailing_diamond": "<p:white>\uf101</>",
          "properties": {
            "prefix": " <#fff>\ueba1</>  ",
            "branch_icon": "\ue725 ",
            "display_status": true,
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 ",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} "
          },
          "style": "diamond",
          "type": "git"
        },
        {
          "foreground": "#0e9c45",
          "background": "transparent",
          "trailing_diamond": "<#0e9c45>\uf101</>",
          "template": " \ue718 {{ .Full }} ",
          "style": "diamond",
          "type": "node"
        },
        {
          "type": "angular",
          "style": "diamond",
          "trailing_diamond": "<#ff4c8b>\uf101</>",
          "foreground": "#ff4c8b",
          "background": "transparent",
          "template": " Óùì {{ .Full }} "
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "trailing_diamond": "<#23deff>\uf101</>",
          "foreground": "#23deff",
          "background": "transparent",
          "template": " \ue77f {{ .Full }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "<#fff700>\uf101</>",
          "foreground": "#fff700",
          "background": "transparent",
          "template": " \ue606 {{ .Full }} ",
          "properties": {
            "home_enabled": true,
            "display_mode": "context",
            "display_default": true
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": "<#fff>\uf101</>",
          "foreground": "#ffffff",
          "background": "transparent",
          "template": " \ue256 {{ .Major }} "
        },
        {
          "type": "php",
          "style": "diamond",
          "trailing_diamond": "<#ff5234>\uf101</>",
          "foreground": "#d6a7ff",
          "background": "transparent",
          "template": " \ue73d {{ .Major }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "trailing_diamond": "<#ff642c>\uf101</>",
          "foreground": "#ff642c",
          "background": "transparent",
          "template": " \ue68b {{ .Full }} "
        },
        {
          "background": "transparent",
          "foreground": "#ff9292",
          "powerline_symbol": "",
          "properties": {
            "template": "\ue256 {{ .Full }}"
          },
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#ff5050",
          "powerline_symbol": "",
          "properties": {
            "template": " \ufd31 {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} "
          },
          "style": "powerline",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ff5a5a",
          "background": "transparent",
          "trailing_diamond": "",
          "template": "{{ if gt .Code 0 }} \uf0f3 {{ end }}"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "<#A3BE8C>[</>",
          "trailing_diamond": "<#A3BE8C>] </>",
          "powerline_symbol": "",
          "background": "transparent",
          "foreground": "#A3BE8C",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "template": ""
          }
        }
      ],

      "type": "rprompt"
    },

    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:white",
          "foreground_templates": ["{{ if gt .Code 0 }}#ff0000{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f_ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": false,
  "version": 3
}
