{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#3aafd6",
    "green": "#8ec07c",
    "yellow": "#d79921",
    "red": "#cc241d",
    "dark": "#3c3836",
    "white": "#ffecc7",
    "os": "p:blue",
    "closer": "p:blue"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "p:yellow",
          "background": "transparent",
          "template": "{{ if .Venv }}({{ .Venv }}){{ end }} ",
          "properties": {
            "home_enabled": true,
            "display_mode": "environment",
            "display_default": true,
            "display_virtual_env": true,
            "folder_name_fallback": false
          }
        },
        {
          "background": "transparent",
          "foreground": "p:os",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "mint": "\uf30f",
            "macos": "\uf179",
            "elementary": "\uF309",
            "raspbian": "\uF315",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\ue70f",
            "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}"
          },
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "properties": {
            "display_host": true
          },
          "style": "diamond",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:green",

          "properties": {
            "folder_icon": "\uf07b ",
            "folder_separator_icon": "\uf443 ",
            "style": "folder"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }} ",
          "trailing_diamond": " ",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "p:white",
          "trailing_diamond": " ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} ",
          "style": "diamond",
          "type": "git"
        },

        {
          "foreground": "#0e9c45",
          "background": "transparent",
          "trailing_diamond": " ",
          "template": " \ue718 {{ .Full }} ",
          "style": "diamond",
          "type": "node"
        },
        {
          "type": "angular",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#ff4c8b",
          "background": "transparent",
          "template": " Óùì {{ .Full }} "
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#23deff",
          "background": "transparent",
          "template": " \ue77f {{ .Full }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#fff700",
          "background": "transparent",
          "template": " \ue606 {{ .Major }}.{{ .Minor }} ",
          "properties": {
            "home_enabled": true,
            "display_mode": "context",
            "display_default": true,
            "display_virtual_env": true,
            "folder_name_fallback": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#ffffff",
          "background": "transparent",
          "template": " \ue256 {{ .Major }} "
        },
        {
          "type": "php",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#d6a7ff",
          "background": "transparent",
          "template": " \ue73d {{ .Major }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "trailing_diamond": " ",
          "foreground": "#ff642c",
          "background": "transparent",
          "template": " \ue68b {{ .Full }} "
        },
        {
          "background": "transparent",
          "foreground": "#ff9292",
          "powerline_symbol": "",
          "properties": {
            "template": "\ue256 {{ .Full }}"
          },
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#ff5050",
          "powerline_symbol": "",
          "properties": {
            "template": " \ufd31 {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} "
          },
          "style": "powerline",
          "type": "kubectl"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:yellow",
          "background": "transparent",
          "template": "\uf101"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ff5a5a",
          "background": "transparent",
          "trailing_diamond": "",
          "template": "{{ if gt .Code 0 }} \uf0f3 {{ end }}"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "<#A3BE8C>[</>",
          "trailing_diamond": "<#A3BE8C>] </>",
          "powerline_symbol": "",
          "background": "transparent",
          "foreground": "#646c5e",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "template": ""
          }
        }
      ],

      "type": "prompt"
    },

    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "p:white",
          "foreground_templates": ["{{ if gt .Code 0 }}#ff0000{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": false,
  "version": 3
}
